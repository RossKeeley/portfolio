<section class="projects">
  <div class="title">
    <h2>Projects</h2>
    <div class="description">
      <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
    </div>
  </div>
  <div class="container">
    <% for (let i = 0; i < projectsData.length; i++) { %>
      <div class="card">
        <img src="<%= projectsData[i].image; %>">
        <h3 class="card-title"><%= projectsData[i].title; %></h3>
        <div class="description">
          <p><%= projectsData[i].description; %></p>
        <!-- <div id="tags"> -->
          <% for (let j = 0; j < projectsData[i].tags.length; j++) { %>
          <span class="tags"><%=  projectsData[i].tags[j]; %></span>
          <% } %>
        <!-- </div> -->
          <a href="/projects/<%= projectsData[i].buttonLink %>" class="btn">Demo</a>
        </div>
      </div>
    <% } %>
  </div>
</section>
