<!DOCTYPE html>
<html lang="en">
  <head>
    <% if (template === 'pomodoro-clock') { %>
      <%- include('partials/pomodoro-links'); %>
    <% } else if (template === 'random-quote-generator') { %>
      <%- include('partials/random-quote-links'); %>
    <% } else { %>
      <%- include('partials/head'); %>
    <% } %>
  </head>
  <body>
    <main>
      <%- include('partials/header'); %>
      <%- include(`pages/${template}`) %>
    </main>
    <footer>
        <%- include('partials/footer'); %>
    </footer>
    <% if (template !== 'pomodoro-clock' && template !== 'random-quote-generator') { %>
      <%- include(`../public/javascripts/${javascript}.js`); %>
    <% } else { %>
      <%- include('../public/javascripts/main.js'); %>
    <% } %>
  </body>
</html>
